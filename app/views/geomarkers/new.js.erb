$("#map").append("<div id='geomarker-form'><%= j render 'geomarkers/new_form' %></div><iframe id='blank_iframe' name='blank_iframe'></iframe>");
$("#cancelFormButton").on("click", function(){
  enableMap();
  quitGeomarkerForm();
  removeIFrame();
});
$("#new_geomarker").on("submit", function(){
  $("#blank_iframe").on("load", function(){
    $(this).remove();
    updateMap();
  });
  quitGeomarkerForm();
});
