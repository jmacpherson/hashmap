<div id="map"></div> 
<div class="map-wrapper">
  
  <div class="map-overlay">
    <a href="#right-menu"><i class="open-menu fi-list"></i></a>
  </div>
</div>

<nav id="right-menu">
  <ul>
    <% @collections.each do |collection| %>
      <li>
        <a href="#"><%= collection.name %></a>
        <ul>
          <% collection.tag_list.each do |tag| %>
          <li><a href="#"><%= tag %></a></li>
          <% end %>
        </ul>
      </li>
    <% end %>
  </ul>
</nav>




